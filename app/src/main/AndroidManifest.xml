<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.benchmate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Setup" />
        <activity android:name=".activity.Load" />
        <activity android:name=".activity.AddReagent" />
        <activity android:name=".activity.Plate" />
        <activity android:name=".activity.Reagents" />
        <activity android:name=".activity.Notes" />
        <activity android:name=".activity.Procedure" />
        <activity android:name=".activity.Settings" />
    </application>

</manifest>